  },
  "extra": {
      "laravel": {
          "providers": [
              "Shahnewaz\\RedprintUnity\\ServiceProvider"
          ]
      }
  },
  "require": {
    "laravel/framework": "^5.6",
    "php": ">=7.1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  }
}
