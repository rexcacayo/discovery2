
{
  "name": "shah-newaz/redprint-unity",
  "description": "Integrate Unity Theme with Redprint.",
  "keywords": ["laravel", "redprint", "unity", "administrator"],
  "license": "MIT",
  "authors": [
    {
      "name": "Shahnewaz Rifat",
      "email": "the.tintin.boss@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Shahnewaz\\RedprintUnity\\": "src/"
    },
    "files": [
        "src/helpers/general.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
